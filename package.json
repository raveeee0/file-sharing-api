{
  "name": "share-post",
  "version": "1.0.0",
  "description": "restful api serving a sharing post web service",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "dev": "npm run build && concurrently \"tsc --watch --build\" \"nodemon ./dist/server.js\"",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raveeee0/share-post.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raveeee0/share-post/issues"
  },
  "homepage": "https://github.com/raveeee0/share-post#readme",
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)",
      "**/test/openapi/routes/**/*.[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.4",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "axios": "^1.6.8",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "jest-openapi": "^0.14.1",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
